{	
	"manifest_version": 2,
	"name": "Inactivity Reset",
	"author": "Jim Adamson", 
	"description": "Pop up a notification and resets the browser after a minute of inactivity",
	"version": "1.3",
	"version_name": "LOPAC-specific version",
	"background": {
		"persistent": true,
		"scripts": [ "background.js" ]
	},
	"minimum_chrome_version": "40",
	"permissions": [
    "browsingData",
		"history",
    "idle",
    "notifications"
	],
	"icons": { 
        "48": "1429370080_sign-out-black-48.png",
        "128": "1429370057_sign-out-black-128.png" 
	}
}
