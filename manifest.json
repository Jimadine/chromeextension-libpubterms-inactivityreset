{
  "manifest_version": 2,
  "name": "Inactivity Reset",
  "author": "Jim Adamson",
  "description": "Pop up a notification and resets the browser after a defined period of inactivity",
  "version": "0.0.0.1",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true,
    "open_in_tab": true
  },
  "background": {
    "persistent": true,
    "scripts": [ "background.js" ]
  },
  "minimum_chrome_version": "40",
  "permissions": [
    "browsingData",
    "history",
    "idle",
    "notifications",
    "storage"
  ],
  "icons": {
        "48": "1429370080_sign-out-black-48.png",
        "128": "1429370057_sign-out-black-128.png"
  }
}
