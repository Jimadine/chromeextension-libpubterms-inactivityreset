{
  "manifest_version": 3,
  "name": "Inactivity Reset",
  "author": "Jim Adamson",
  "description": "Pop up a notification and reset the browser after a defined period of inactivity",
  "version": "0.0.0.7",
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": true
  },
	"background": {
    "service_worker": "scripts/service_worker.js"
  },
  "minimum_chrome_version": "126",
  "permissions": [
    "alarms",
    "browsingData",
    "history",
    "idle",
    "notifications",
    "storage"
  ],
  "icons": {
        "48": "icons/1429370080_sign-out-black-48.png",
        "128": "icons/1429370057_sign-out-black-128.png"
  }
}
